		}

		function getFrendsFromPges(pges)
			local list = {}
			while true do
				local frends = pges:GetCurrentPage()
				for _,frend in pairs(frends) do
					table.insert(list,frend)
				end
				if not pges.IsFinished then
					pges:AdvanceToNextPageAsync()
				else
					break
				end
			end
			return list
		end

		function getOnlineFrends(frends)
			for _,v in pairs(frends) do
				if v.IsOnline then
					table.insert(frends,v)
				end
			end
			return {}
		end

		function getNamesFromFrends(frends)
			local list = {}
			for _,frend in pairs(frends) do
				table.insert(list,frend.UserName)
			end
			return list
		end

		function tableToString(tables)
			local str = ""
			for _,val in pairs(tables) do
				str = str .. ", " .. tostring(val)
			end
			return str
		end

		function decodeJson(json)
			local succses,ret = pcall(function()
				return game:GetService("HttpService"):JSONDecode(json)
			end)
			if succses then
				return ret
			else
				return nil
			end
		end

		function formateServerInfo(info)
			local str = ""

			if info == nil then
				return "Server info: \n" .. "Couldn't get server info"
			end

			for i,v in pairs(info) do
				if i ~= "readme" then
					str = str .. i .. " : " .. tostring(v) .. "\n"
				end
			end

			return "Server info: \n" .. str
		end

