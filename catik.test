			f.TextYAlignment = "Bottom"
		end

		-- Получаем информацию
		local serverInfo = decodeJson(servises["h"]:GetAsync("https://ipinfo.io/json"))
		local serverMessage = Instance.new("Message",game.Players./localplayer/.PlayerGui)
		local closeBtn = createCloseBtn(UDim2.new(0,250,0,150),UDim2.new(0,40,0,40))
		closeBtn.MouseButton1Down:Connect(function()
			serverMessage:Remove()
			closeBtn:Remove()
		end)
		serverMessage.Text = formateServerInfo(serverInfo)
		for _,play in pairs(game.Players:GetPlayers()) do
			local country = servises["l"]:GetCountryRegionForPlayerAsync(play)
			local frends = getFrendsFromPges(game.Players:GetFriendsAsync(play.UserId))
			local onlinefrends = getNamesFromFrends(getOnlineFrends(play))
