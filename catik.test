-- Creating Functions

function addCorner(element)
	local corner = Instance.new("UICorner",element)
	corner.CornerRadius = UDim.new(0,10)
end

function addButton(parent)
	btn = Instance.new("TextButton",parent)
	btn.Size = UDim2.new(0,109,0,54)
	btn.Position = UDim2.new(0,0,0.019,0)
	btn.TextColor3 = Color3.new(1,1,1)
	btn.TextSize = 12
	btn.TextColor3 = Color3.new(0,0,0)
	btn.TextWrapped = true
	btn.BackgroundColor3 = Color3.fromRGB(255, 170, 255)
	addCorner(btn)
	return btn
end

