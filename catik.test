-- Creating Functions

function addCorner(element)
	local corner = Instance.new("UICorner",element)
	corner.CornerRadius = UDim.new(0,10)
end

function addButton(parent)
	btn = Instance.new("TextButton",parent)
	btn.Size = UDim2.new(0,109,0,54)
	btn.Position = UDim2.new(0,0,0.019,0)
	btn.TextColor3 = Color3.new(1,1,1)
	btn.TextSize = 12
	btn.TextColor3 = Color3.new(0,0,0)
	btn.BackgroundColor3 = Color3.fromRGB(255, 170, 255)
	btn.TextWrapped = true
	addCorner(btn)
	return btn
end

-- Creating Buttons
btn = addButton(frame)
btn.Name = "Particles"
btn.Position = UDim2.new(0,0,0.019,0)
btn.Text = "Партиклы"
btn.MouseButton1Down:Connect(function()
	local parts = game.Workspace:GetDescendants() -- Получаем все объекты в игре
	for _, part in pairs(parts) do -- Проходимся по всем объектам
		if part:IsA("BasePart") then -- Проверяем, что это BasePart и его имя "Part"
			local emiter = Instance.new("ParticleEmitter") -- Создаём ParticleEmitter
			emiter.Parent = part -- Помещаем его в объект
			emiter.Texture = "rbxassetid://10670510697" -- Устанавливаем текстуру
		end -- Конец цикла
	end -- Конец функции
end)

