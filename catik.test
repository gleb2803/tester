function isValidFunction(funName)
	local valid = false
	for i,v in pairs(functions) do
		if i == funName then
			valid = true
		end
	end
	return valid
end

function getNames(name)
	local list = {"Empty","Пусто Выросла Копуста"} -- Стандартное Имя
	
	for i,v in Names do
		if i == name then
			list = v
		end
	end
	
	return list
end


-- Creating Buttons
for p = 1,3 do
	local currentPage = Instance.new("Frame",pges)
	currentPage.Name = "Page"..p
	currentPage.Position = UDim2.new(0,0,0,0)
	currentPage.Size = UDim2.new(0,366,0,310)
	currentPage.BackgroundTransparency = 1
	if p ~= 1 then currentPage.Visible = false end
	for i = 1,3 do
		for v = 1,4 do
			btn = addButton(currentPage)
			btn.Position = UDim2.new(0,0 + ((i - 1) * 121),0,7 + ((v - 1) * 63))
			btn.Text = getNames("Function"..p..i..v)[2]
			btn.Name = getNames("Function"..p..i..v)[1]
			btn.MouseButton1Down:Connect(function()
				if isValidFunction(getNames("Function"..p..i..v)[1]) then
					functions[getNames("Function"..p..i..v)[1]]() -- Не Трогать Это Говно А то все сломается
				end
			end)
		end
	end
end


local curPage = 1

function changePage(pageNum)
	local newPage = pges:FindFirstChild("Page"..pageNum)
	if newPage then
		for _, page in pairs(pges:GetChildren()) do
			page.Visible = false
		end
		newPage.Visible = true
		curPage = pageNum
	end
end

nextBtn.MouseButton1Down:Connect(function()
	if curPage == #pges:GetChildren() then
		changePage(1)
	else
		changePage(curPage + 1)
	end
end)
